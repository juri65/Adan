<h4>投稿一覧</h4>
<div class="container">
    <div class="row">
        <% @posts.each do |post| %>
        <div class="col-3 ">
            <%= link_to post_path(post.id) do %>
            <%= image_tag post.get_image(200,200) %>
            <% end %>
        </div>
        <% end %>
    </div>
</div>
